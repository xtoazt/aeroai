# API Keys for Oumi Vercel Deployment
# Copy this file to .env.local and fill in your actual API keys

# Required: OpenAI API Key (for basic functionality)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Google Gemini API Key (for Gemini models)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Together AI API Key (for open source models)
TOGETHER_API_KEY=your_together_api_key_here

# Optional: Debug mode
DEBUG=false
